cd CompetitionsResults/
for i in $(find -maxdepth 1 -type d -name 'IPC*' );
do
    echo "Directiorio: $i"
    cd $i/domains
    for j in $(find -maxdepth 1 -type d );
    do
	cd $j
	for k in $(find -maxdepth 1 -type f -name '*.pddl' );
	do
	    echo "Ejecuando: $i/domains$j$k"
	done
	#echo "output: $i"
    done
done
